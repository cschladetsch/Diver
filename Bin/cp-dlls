#!/bin/sh

repo_name=Liminal_UI_v2
project_name='LiminalApp-v2'

src_dir=Debug
target_dir=$WORK_DIR/$repo_name/$project_name/Assets/Plugins/Liminal

dll_names=( Core ExecutionContext LanguageCommon Network PiLang RhoLang Executor )

for dll in ${dll_names[@]}; 
do
    name=Pyro.$dll.dll
    src=$src_dir/$name
    dest=$target_dir/$name
    echo Copying $name
    cp $src $dest
done

